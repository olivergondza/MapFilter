<phpunit
    colors="true"
    bootstrap="bootstrap.php">
  <testsuites>
    <testsuite name="MapFilter">
      <directory suffix=".php">.</directory>
    </testsuite>
  </testsuites>
  
  <filter>
    <whitelist>
      <directory suffix=".php">../..</directory>
    </whitelist>
  </filter>
</phpunit>
